{
    "manifest_version": 2,
    "name": "demo",
    "version": "1.0.0",
    "description": "This extension is a demo on this page",
    "icons": {
      "128": "circular-arrows.png",
      "32": "circular-arrows.png",
      "48": "circular-arrows.png"
    },
    "browser_action": {
      "default_icon": {
        "16": "circular-arrows.png",
        "32": "circular-arrows.png"
      },
      "default_popup": "popup.html",
      "default_title": "demo"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ]
  }
  